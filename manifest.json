{
  "name": "Bypass Redirect Notice",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Bypass redirect notice on emails",
  "homepage_url": "https://github.com/yilmazaurea/chrome-bypass-redirect-notice",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/url*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}